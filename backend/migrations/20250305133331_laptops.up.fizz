create_table("laptops") {
    t.Column("id", "integer", {primary: true})
    t.Column("model_name", "string")
    t.Column("processor", "string")
    t.Column("ram", "integer")
    t.Column("storage", "integer")
    t.Column("display", "string")
    t.Column("price", "integer")
    t.Column("company", "string")
    t.Column("image_link", "string")
}